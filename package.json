    "boxrec-mocks": "https://github.com/boxing/boxrec-mocks",
    "cheerio": "^1.0.0-rc.2",
    "coveralls": "^3.0.0",
    "jest": "^22.4.3",
    "json-loader": "^0.5.7",
    "request": "^2.83.0",
    "ts-jest": "^22.0.3",
    "ts-loader": "4.1.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.7.1",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "webpack": "4.4.1",
    "webpack-cli": "^2.0.13",
