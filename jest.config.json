{
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "spec\\.ts",
  "moduleFileExtensions": [
    "ts",
    "js",
    "json"
  ],
  "moduleNameMapper":{
    "^@helpers$": "<rootDir>/src/helpers.ts",
    "^@boxrec-pages/(.*)": "<rootDir>/src/boxrec-pages/$1",
    "^@boxrec-common-tables/(.*)": "<rootDir>/src/boxrec-common-tables/$1",
    "^@boxrec-constants$": "<rootDir>/src/boxrec-pages/boxrec.constants.ts"
  }
}
